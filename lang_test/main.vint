import net
import time

package main{
    name = "Vint"

    let s = name.split("")
    for i in s{
        print(i)

    }

    let age = "20"

    height = "6.0" //in feats

    print("my name is "+name)

    let printStmt = func(name,age,height){
        print("My name is "+name+" I am "+age+" old and my height is "+height)
    }

    //calling the function 
    printStmt(name,age,height)

    height = "7"
    printStmt(name,age,height)
    print(time.now())

    let greet = func(name) {
        let hour = time.now().hour()
        print(hour)
        if (hour < 12){
            print("Good morning, " + name + "!")
        }
        else {
            print("Good evening, " + name + "!")
        }
    }
    greet()
}

